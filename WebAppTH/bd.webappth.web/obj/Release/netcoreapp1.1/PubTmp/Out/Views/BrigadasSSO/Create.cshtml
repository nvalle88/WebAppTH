@model bd.webappth.entidades.Negocio.BrigadaSSO

@{
    ViewData["Title"] = "BrigadasSSO";
}


			<!-- MAIN CONTENT -->
<div id="content">

    <!-- widget grid -->
    <section id="" class="">
        <div class="row">
            <article class="col-sm-12 col-md-12 col-lg-7">
                <div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">
                    <header id="table">
                        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                        <h2>Brigadas de salud y seguridad ocupacional</h2>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox">
                        </div>
                        <div class="widget-body no-padding">
                            <form  asp-action="Create" id="checkout-form"  class="smart-form" novalidate="novalidate">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                @{await Html.RenderPartialAsync("_PartialViewBrigadasSSO", Model);}                           
                                <footer>           
                                    <input type="submit" value="Guardar" id="btn-guardar" class="btn btn-primary" />
                                    <a asp-action="Index"  class="btn btn-default">Cancelar</a>
                                </footer>
                            </form>
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>
@section Scripts {
    <script type="text/javascript">


    </script>
}
