                                success: function (data) {
                                    if (data == "true") {
                                        $('#dt_basic tr:contains("' + id + '")').remove();
                                    }
                                    else {
                                        alert(data);
                                    }
                                },

                            });
                        });
                    }
                    else
                    {
                        alert("Existe un registro con igual informacion o no todos los campos estan llenos");
                    }

                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
                   })

