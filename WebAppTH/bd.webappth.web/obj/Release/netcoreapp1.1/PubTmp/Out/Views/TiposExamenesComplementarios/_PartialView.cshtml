@model bd.webappth.entidades.Negocio.TipoExamenComplementario

<fieldset>
    <div class="row">
        
        <section class="col col-4">
            <label class="input">
                <p>@Html.DisplayNameFor(model => model.Nombre)</p>
                <input maxlength="50" asp-for="Nombre" class="form-control" placeholder="@Html.DisplayNameFor(model=>model.Nombre)" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </label>
        </section>
    </div>
    
</fieldset>