<script>

    var chk1,chk2;

    function ocultarAccidenteTrabajoSeccion() {
        $("#EmpresaAccidenteSeccion").hide(); 

        chk1 = false;

        if (chk2 == false) {
            $("#DetalleAntecedenteProfesional").hide();

        }
        
    }

    function mostrarAccidenteTrabajoSeccion() {

        chk1 = true;

        $("#EmpresaAccidenteSeccion").show();
        $("#DetalleAntecedenteProfesional").show();
        
    }

    function ocultarEnfermedadProfesionalSeccion() {
        
        $("#EnfermedadProfesionalSeccion").hide();

        chk2 = false;

        if (chk1 == false) {
            $("#DetalleAntecedenteProfesional").hide();
        }
        
    }

    function mostrarEnfermedadProfesionalSeccion() {

        chk2 = true;

        $("#EnfermedadProfesionalSeccion").show();
        $("#DetalleAntecedenteProfesional").show();
        
    }

    function mostrarSeccion(IdSeccion) {

        var IdObjeto = "#" + IdSeccion;

        $(IdObjeto).show();

    }

    function ocultarSeccion(IdSeccion) {

        var IdObjeto = "#" + IdSeccion;

        $(IdObjeto).hide();

    }

    function clickIconoAntecedenteGinecoObsterico(idIcono) {

        var idIconoIngresado = "#" + idIcono;

        if ($(idIconoIngresado).attr("class") == "Editar-icono") {

            $(idIconoIngresado).attr("class", "Editar-icono-activado");
            mostrarSeccion("AntecedenteGinecoObstericoSeccion");

        } else {

            $(idIconoIngresado).attr("class", "Editar-icono");
            ocultarSeccion("AntecedenteGinecoObstericoSeccion");
        }
        
    }
    

    function clickIconoAntecedentePersonal(idIcono) {

        var idIconoIngresado = "#" + idIcono;

        if ($(idIconoIngresado).attr("class") == "Editar-icono") {

            $(idIconoIngresado).attr("class", "Editar-icono-activado");
            mostrarSeccion("AntecedentePersonalSeccion");

        } else {

            $(idIconoIngresado).attr("class", "Editar-icono");
            ocultarSeccion("AntecedentePersonalSeccion");
        }

    }

    function mostrarCigarrilloSeccion() {
        mostrarSeccion("CigarrilloSeccion");

    }
    
    function ocultarCigarrilloSeccion() {
        ocultarSeccion("CigarrilloSeccion");
    }

    function mostrarLicorSeccion() {
        mostrarSeccion("LicorSeccion");

    }

    function ocultarLicorSeccion() {
        ocultarSeccion("LicorSeccion");
    }

    function mostrarDrogasSeccion() {
        mostrarSeccion("DrogasSeccion");

    }

    function ocultarDrogasSeccion() {
        ocultarSeccion("DrogasSeccion");
    }

    function mostrarEjerciciosSeccion() {
        mostrarSeccion("EjerciciosSeccion");

    }

    function ocultarEjerciciosSeccion() {
        ocultarSeccion("EjerciciosSeccion");
    }

    function mostrarSospechaEnfermedadLaboralSeccion() {
        mostrarSeccion("SospechaEnfermedadLaboralSeccion");
    }

    function ocultarSospechaEnfermedadLaboralSeccion() {
        ocultarSeccion("SospechaEnfermedadLaboralSeccion");
    }
    



    onload = function () {

        chk1 = false;
        chk2 = false;

        ocultarAccidenteTrabajoSeccion();
        ocultarEnfermedadProfesionalSeccion();

        ocultarSeccion("AntecedentePersonalSeccion");
        ocultarSeccion("AntecedenteGinecoObstericoSeccion");

        ocultarCigarrilloSeccion();
        ocultarLicorSeccion();
        ocultarDrogasSeccion();
        ocultarEjerciciosSeccion();

        ocultarSospechaEnfermedadLaboralSeccion();
    }

</script>