@model bd.webappth.entidades.Negocio.FichaMedica



<style>

    .Editar-icono {

        border-radius:0px;

        height: 40px;
        width: 40px;

        color:black;
        background-color: rgba(255,255,255,1);
        background-repeat: no-repeat;
        background-size: 100% 100%;

        padding:5px;

        float: right;
    }

    .Editar-icono-activado {
        border-radius: 0px;
        height: 40px;
        width: 40px;
        color: black;
        background-color: rgb(255, 189, 33);
        background-repeat: no-repeat;
        background-size: 100% 100%;
        padding: 5px;
        float: right;
    }

    .Editar-icono:hover{
        background-color: rgb(255, 189, 33);
    }

    .contenedor-cabecera {
        height: 50px;
        background-color:black;
        color:white;
        text-align:left;
    }

    .contenedor-cabecera .titulo{
        padding-top:15px;
        padding-left:15px;

        float:left;
    }

    section{
        /*background-color:rgb(132,132,132);*/
        
        color: white;
        font-size: 15px;

        overflow:hidden;
    }

    section .contenedor-seccion{
        margin:2%;
        
    }

    .fondo-seccion {
        background-color: rgba(142,194,219,1);
    }

    p{
        font-weight: bold;
    }


    .contenedor-seccion2 {
        margin-left: 0%;
        margin-right: 0%;

        color: white;
        font-size: 15px;

        float: none;
    }
    .columna1{

        padding-left:2%;

        width: 18%;
        height:27px;
        background-color: rgba(142,194,219,1);
        float: left;

    }

    .columna2 {
        width: 10%;
        height: 27px;
        float: left;
        background-color: rgba(142,194,219,0.7);
        text-align: center;
    }

    .columna3 {
        width: 10%;
        height: 27px;
        float: left;
        background-color: rgba(142,194,219,1);
        text-align: center;
    }

    .columna4 {
        width: 58%;
        height: 27px;
        padding-left: 2%;
        background-color: rgba(142,194,219,0.7);
        float: right;
    }

    .columna4 input {
        height:23px;
    }

