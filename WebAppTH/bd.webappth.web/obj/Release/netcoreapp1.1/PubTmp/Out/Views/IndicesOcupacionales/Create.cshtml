    <script type="text/javascript">
        $(document).ready(function () {

           $("#ciudad").change(function () {
            mostrarLoadingPanel("content","Cargando...")
            $("#sucursal").empty();
            $("#dependencia").val("");
            $("#manualpuesto").empty();
            $("#rolpuesto").empty();
            $("#escalagrados").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarSucursalesPorCiudad", "CeseFunciones")',
                    dataType: 'json',
                    data: { idciudad: $("#ciudad").val() },
                    success: function (data) {
                        $("#sucursal").append('<option value="0" selected="" disabled="">Seleccione</option>');
                        $.each(data, function (i, data) {
                            $("#sucursal").append('<option value="'
                                + data.idSucursal + '">'
                                + data.nombre + '</option>');

                        });
                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
                })

           $("#sucursal").change(function (e) {
               mostrarLoadingPanel("content", "Cargando...")
               $("#dependencia").val("");
               $("#manualpuesto").empty();
               $("#rolpuesto").empty();
               $("#escalagrados").empty();
               e.preventDefault();
               var $link = $(this);
               var title = $link.text();
               $('#myModal.modal-title').html(title);
               var url = '@Url.Action("CargarDependencias", "Dependencias")?idsucursal='+ $('#sucursal').val() ;
               if (url.indexOf('#') == 0) {
                   $('#myModal').modal('show');
                   $("#myModal").waitMe("hide");
               }
               else {
                   $.get(url, function (data) {
                       $('#myModal .te').html(data);
                       $('#myModal').modal();
                       $('#selectC').select2();
                       $("#content").waitMe("hide");
                   }).success(function () { $('input:text:visible:first').focus(); });

               }
                $("#content").waitMe("hide");
                return false;
           })




                        $("#manualpuesto").change(function () {
            mostrarLoadingPanel("content","Cargando...")
            $("#rolpuesto").empty();
            $("#escalagrados").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarRolPuestoporManualPuesto")',
                    dataType: 'json',
                    data: {
                        idmanualpuesto: $("#manualpuesto").val(),
                        iddependencia: $("#iddependencia").val(),
                        },
                    success: function (data) {
                        $("#rolpuesto").append('<option value="0" selected="" disabled="">Seleccione</option>');
                        $.each(data, function (i, data) {
                            $("#rolpuesto").append('<option value="'
                                + data.rolPuesto.idRolPuesto + '">'
                                + data.rolPuesto.nombre + '</option>');

                        });
                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
                        })


                        $("#rolpuesto").change(function () {
                            mostrarLoadingPanel("content", "Cargando...")
                            $("#escalagrados").empty();
                            $.ajax({
                                type: 'POST',
                                url: '@Url.Action("ListarEscalaGradosPorRolPuesto")',
                                dataType: 'json',
                                data: {
                                    idmanualpuesto: $("#manualpuesto").val(),
                                    iddependencia: $("#iddependencia").val(),
                                    idrolpuesto: $("#rolpuesto").val(),
                                },
                                success: function (data) {
                                    if (data[0].escalaGrados.grado !== null) {
                                        $("#escalagrados").append('<option value="0" selected="" disabled="">Seleccione</option>');
                                        $.each(data, function (i, data) {

                                            $("#escalagrados").append('<option value="'
                                                + data.escalaGrados.idEscalaGrados + '">'
                                                + data.escalaGrados.nombre + " (Grado " + data.escalaGrados.grado + ")" + " que percibe" + " $" + data.escalaGrados.remuneracion + '</option>');
                                        });
                                    }
                                    else {
                                        $("#escalagrados").append('<option value="'
                                            + 0 + '">'
                                            + "No aplica" + '</option>');

                                    }
                                }, complete: function (data) {
                                    $("#content").waitMe("hide");
                                },

                                error: function (ex) {
                                    alert('Failed to retrieve data.' + ex);
                                }
                            });
                            return false;
                        });

                        $("#idgrupoocupacional").change(function () {
                            mostrarLoadingPanel("content", "Cargando...")
                            $("#idescalagrados").empty();
                            $.ajax({
                                type: 'POST',
                                url: '@Url.Action("ListarEscalasGradosPorGrupoOcupacional", "EscalaGrados")',
                                dataType: 'json',
                                data: { idgrupoocupacional: $("#idgrupoocupacional").val() },
                                success: function (data) {
                                    $("#idescalagrados").append('<option value="0" selected="" disabled="">Seleccione</option>');
                                    $.each(data, function (i, data) {
                                        $("#idescalagrados").append('<option value="'
                                            + data.idEscalaGrados + '">'
                                            + data.nombre + '</option>');

                                    });
                                }, complete: function (data) {
                                    $("#content").waitMe("hide");
                                },

                                error: function (ex) {
                                    alert('Failed to retrieve data.' + ex);
                                }
                            });
                            return false;
                        });

        });
    </script>
