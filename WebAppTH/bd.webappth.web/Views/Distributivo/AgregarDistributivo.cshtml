    <script src="//cdnjs.cloudflare.com/ajax/libs/jstree/3.3.5/jstree.min.js"></script>*@

<!-- MAIN CONTENT -->
<div id="content">

    <!-- widget grid -->
    <section id="widget-grid" class="">

        <!-- START ROW -->
        <div class="row">
            <!-- NEW COL START -->
            <article class="col-sm-12 col-md-12 col-lg-12">
                <!-- Widget ID (each widget will need unique ID)-->
                <div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">

                    <header>
                        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                        <h2> Distributivo</h2>
                    </header>
                    <!-- widget div-->
                    <div>

                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body no-padding">

                            <form asp-action="AgregarADistributivo" asp-controller="Empleados" class="smart-form">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="Empleado.IdEmpleado" />
                                <input type="hidden" asp-for="IndiceOcupacional.IdIndiceOcupacional" id="txt_idIO" />

                                <fieldset>

                                    <div class="row" hidden>
                                        <input type="text" asp-for="IndiceOcupacionalModalidadPartida.IdIndiceOcupacionalModalidadPartida" />
                                    </div>

                                    <div class="row">
                                        <section class="col col-4">
                                            <label>R&eacute;gimen laboral</label>
                                            <label class="input">
                                                <select id="IdRegimenLaboral" class="form-control" asp-items="ViewBag.IdRegimenLaboral" data-msg-required='El campo es requerido' data-rule-required='true' asp-for="IndiceOcupacionalModalidadPartida.TipoNombramiento.RelacionLaboral.IdRegimenLaboral" disabled>
                                                    <option value="0" selected="" disabled="">Seleccione</option>
                                                </select>
                                            </label>
                                        </section>
                                        <section class="col col-4">
                                            <label>Relaci&oacute;n laboral</label>
                                            <label class="input">
                                                <select id="IdRelacionLaboral" class="form-control" asp-items="ViewBag.IdRelacionLaboral" data-msg-required='El campo es requerido' data-rule-required='true' asp-for="IndiceOcupacionalModalidadPartida.TipoNombramiento.IdRelacionLaboral" disabled>
                                                    <option value="0" selected="" disabled=""></option>
                                                </select>
                                            </label>
                                        </section>
                                        <section class="col col-4">
                                            <label>Tipo de nombramiento / contrato</label>
                                            <label class="input">
                                                <select id="IdTipoNombramiento" asp-for="IndiceOcupacionalModalidadPartida.IdTipoNombramiento" class="form-control" asp-items="ViewBag.IdTipoNombramiento" data-msg-required='El campo es requerido' data-rule-required='true' disabled></select>
                                                <span asp-validation-for="IndiceOcupacionalModalidadPartida.IdTipoNombramiento" class="text-danger"></span>
                                            </label>
                                        </section>
                                    </div>
                                    <div class="row">
                                        <section class="col col-4">
                                            <label>Ciudad</label>
                                            <label class="input">
                                                <select id="ciudad" class="form-control" asp-items="ViewBag.IdCiudad" asp-for="IndiceOcupacionalModalidadPartida.IndiceOcupacional.Dependencia.Sucursal.IdCiudad" disabled>
                                                    <option value="0" selected="" disabled="">Seleccione</option>
                                                </select>
                                            </label>
                                        </section>
                                        <section class="col col-4">
                                            <label>Sucursal</label>
                                            <label class="input">
                                                <select id="sucursal" asp-for="Dependencia.Sucursal.IdSucursal" asp-items="ViewBag.IdSucursal" class="form-control" data-msg-required='El campo es requerido' data-rule-required='true' disabled></select>
                                                <span asp-validation-for="Dependencia.Sucursal.IdSucursal" class="text-danger"></span>
                                            </label>
                                        </section>
                                        <section class="col col-4">
                                            <label>Dependencia</label>
                                            <label class="input">
                                                <input class="form-control" type="text" id="dependencia" @if (Model.Dependencia != null) { <text> value="@Model.Dependencia.Nombre" </Text> } disabled />

                                            </label>

                                        </section>

                                        <input class="form-control" type="hidden" id="iddependencia" asp-for="IndiceOcupacional.IdDependencia" disabled />
                                        <span asp-validation-for="IndiceOcupacional.IdDependencia" class="text-danger"></span>
                                    </div>
                                    <div class="row">
                                        <section class="col col-4">
                                            <label>Perfil del puesto</label>
                                            <select id="manualpuesto" asp-for="IndiceOcupacional.IdManualPuesto" class="form-control" asp-items="ViewBag.IdManualPuesto" data-msg-required='El campo es requerido' data-rule-required='true' disabled></select>
                                            <span asp-validation-for="IndiceOcupacional.IdManualPuesto" class="text-danger"></span>
                                        </section>
                                        <section class="col col-4">

                                            @*<label>Rol Puesto</label>
                                                <select id="rolpuesto" asp-for="IndiceOcupacional.IdRolPuesto" asp-items="ViewBag.IdRolPuesto" class="form-control"></select>
                                                <span asp-validation-for="IndiceOcupacional.IdRolPuesto" class="text-danger"></span>*@

                                            <label>Rol del puesto</label>
                                            <input class="form-control input" id="txt_rolPuesto" value="@if (Model.IndiceOcupacional != null) { <text>@Model.IndiceOcupacional.RolPuesto.Nombre </text>} else { <text></text>}" readonly disabled />


                                        </section>

                                        <section class="col col-4">
                                            <label>Fondo de Financimiento</label>
                                            <select asp-for="IndiceOcupacionalModalidadPartida.IdFondoFinanciamiento" class="form-control" data-msg-required='El campo es requerido' data-rule-required='true' asp-items="ViewBag.IdFondoFinanciamiento" disabled>
                                                <option value="0" selected="" disabled="">Seleccione</option>
                                            </select>
                                            <span asp-validation-for="IndiceOcupacionalModalidadPartida.IdFondoFinanciamiento" class="text-danger"></span>
                                        </section>

                                    </div>

                                    <div class="row">

                                        <section class="col col-4">

                                            <label>Partida individual / c&oacute;digo</label>
                                            <input asp-for="IndiceOcupacionalModalidadPartida.NumeroPartidaIndividual" class="form-control input" id="txt_partida" disabled>
                                        </section>

                                        <section class="col col-8">

                                            @*<label>Escala Grados</label>
                                                <select id="escalagrados" asp-for="IndiceOcupacional.IdEscalaGrados" asp-items="ViewBag.IdEscala" class="form-control"></select>
                                                <span asp-validation-for="IndiceOcupacional.IdEscalaGrados" class="text-danger"></span>*@


                                            <label>Grupo ocupacional</label>
                                            <input class="form-control input" id="txt_escalaGrados"
                                                   value="@if (Model.IndiceOcupacional != null)
                                                   {
                                                       var texto = Model.IndiceOcupacional.EscalaGrados.Nombre
                                                                  + " ( GRADO "
                                                                  + Model.IndiceOcupacional.EscalaGrados.Grado
                                                                  + " ) QUE PERCIBE $"
                                                                  + Model.IndiceOcupacional.EscalaGrados.Remuneracion;
                                                <text>
                                                   @texto
                                                </text>
                                            } else { <text></text>}"
                                                   readonly required>


                                        </section>



                                    </div>

                                    <div class="row">


                                        <section class="col col-4" id="s_modalidad">
                                            <label>Modalidad Partida</label>
                                            <select id="modalidadpartida" asp-for="IndiceOcupacional.IdModalidadPartida" asp-items="ViewBag.IdModalidadPartida" class="form-control" disabled></select>
                                            <span asp-validation-for="IndiceOcupacional.IdModalidadPartida" class="text-danger"></span>
                                        </section>


                                    </div>

                                    <div class="row">

                                        <section class="col col-4">
                                            <label>Fecha de primer ingreso</label>
                                            <input asp-for="FechaPrimerIngreso" type="text" class="form-control" data-dateformat='dd/mm/yy' data-msg-required='El campo es requerido' data-rule-required='true' disabled>

                                        </section>

                                        <section class="col col-4">
                                            <label>Fecha de ingreso</label>
                                            <input asp-for="IndiceOcupacionalModalidadPartida.Fecha" type="text" class="form-control datepicker " data-dateformat='dd/mm/yy' data-msg-required='El campo es requerido' data-rule-required='true' disabled>
                                            <span asp-validation-for="IndiceOcupacionalModalidadPartida.Fecha" class="text-danger"></span>
                                        </section>

                                        <section class="col col-4">
                                            <label>Fecha fin</label>
                                            <input asp-for="IndiceOcupacionalModalidadPartida.FechaFin" type="text" class="form-control datepicker " data-dateformat='dd/mm/yy' disabled>
                                            <span asp-validation-for="IndiceOcupacionalModalidadPartida.FechaFin" class="text-danger"></span>
                                        </section>


                                    </div>

                                    <div class="row">
                                        <section class="col col-4">
                                            <label class="checkbox-inline">
                                                <label>&iquest; Sobrevalorado ?</label>
                                                <input type="checkbox" asp-for="SalarioReal" class="form-control" disabled/>
                                            </label>
                                        </section>
                                        <section class="col col-4" id="IdsalarioReal">
                                            @*<div class="col-sm-2" id="IdsalarioReal">*@
                                            <label>Salario sobrevalorado</label>
                                            <input asp-for="IndiceOcupacionalModalidadPartida.SalarioReal" class="form-control input " data-msg-required='El campo es requerido' data-rule-required='true' onKeyPress="return checkIt(event)" disabled>
                                            @*</div>*@
                                        </section>

                                        <section class="col col-4">
                                            <label class="checkbox-inline">
                                                <label>&iquest; Es jefe de la unidad administrativa?</label>
                                                <input type="checkbox" asp-for="Empleado.EsJefe" class="form-control" disabled/>
                                            </label>
                                        </section>
                                    </div>
                                    <div class="row">
                                        &nbsp;
                                    </div>
                                </fieldset>
                                <br />
                                <footer>
                                    
                                    <a asp-action="Index" class="btn btn-default">Regresar</a>
                                </footer>
                            </form>
                        </div>
                        <!-- end widget content -->
                    </div>
                    <!-- end widget div -->
                </div>
                <!-- end widget -->
            </article>

        </div>
        <!-- END ROW -->
    </section>
    <!-- end widget grid -->
    <!-- Modal -->
</div>
<!-- END MAIN CONTENT -->
