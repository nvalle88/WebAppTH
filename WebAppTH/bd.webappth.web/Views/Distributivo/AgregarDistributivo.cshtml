@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <!-- PAGE RELATED PLUGIN(S) -->
    <script src="~/js/plugin/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/js/plugin/fuelux/wizard/wizard.min.js"></script>
    <script src='~/js/vistasmodales/modal.js'></script>


    <script type="text/javascript">

        @{
            if (Model.IndiceOcupacional != null ) {

                if (Model.IndiceOcupacional.IdModalidadPartida > 0)
                {

                    <text>
                        document.getElementById("s_modalidad").style.visibility = "visible";
                    </text>

                }
                else {
                    <text>
                        document.getElementById("s_modalidad").style.visibility = "hidden";
                    </text>
                }
            }
        };

        if (!$("#SalarioReal").prop('checked')) {
            $('#IdsalarioReal').hide();

        }
        // DO NOT REMOVE : GLOBAL FUNCTIONS!

        $("#SalarioReal").change(function () {
            if (!$(this).prop('checked')) {
                $('#IdsalarioReal').hide();
            } else {
                $('#IdsalarioReal').show();
            }

        })
        $(document).ready(function () {

            //Bootstrap Wizard Validations

            $("#getSucursal").click(function (e) {
                idsucursal = $("#sucursal").val()
                });



            $('.tree > ul').attr('role', 'tree').find('ul').attr('role', 'group');
            $('.tree').find('li:has(ul)').addClass('parent_li').attr('role', 'treeitem').find(' > span').attr('title', 'Collapse this branch').on('click', function (e) {
                var children = $(this).parent('li.parent_li').find(' > ul > li');
                if (children.is(':visible')) {
                    children.hide('fast');
                    $(this).attr('title', 'Expand this branch').find(' > i').removeClass().addClass('fa fa-lg fa-plus-circle');
                } else {
                    children.show('fast');
                    $(this).attr('title', 'Collapse this branch').find(' > i').removeClass().addClass('fa fa-lg fa-minus-circle');
                }
                e.stopPropagation();
            });

            var $validator = $("#wizard-1").validate({

                rules: {
                    IdTipoIdentificacion: {
                        required: true
                    },
                    Identificacion: {
                        required: true
                    },
                    Nombres: {
                        required: true
                    },
                    Apellidos: {
                        required: true
                    },
                    FechaNacimiento: {
                        required: true
                    },
                    Parentesco: {
                        required: true
                    },
                    IdRelacionLaboral: {
                        required: true
                    },
                    paisLugarNacimiento: {
                        required: true
                    },
                    paisLugarSufragio: {
                        required: true
                    },
                    paisLugarPersona: {
                        required: true
                    },
                    provinciaLugarPersona: {
                        required: true
                    },
                    idciudadLugarPersona: {
                          required: true
                      }
                },

                messages: {
                    IdTipoIdentificacion: "El campo es requerido",
                    Identificacion: "El campo es requerido",
                    Nombres: "El campo es requerido",
                    Apellidos: "El campo es requerido",
                    Nombres: "El campo es requerido",
                    FechaNacimiento: "El campo es requerido",
                    Parentesco: "El campo es requerido",
                    IdRelacionLaboral: "El campo es requerido",
                    paisLugarNacimiento: "El campo es requerido",
                    paisLugarSufragio: "El campo es requerido",
                    paisLugarPersona: "El campo es requerido",
                    provinciaLugarPersona: "El campo es requerido",
                    idciudadLugarPersona: "El campo es requerido",
                    email: {
                        required: "We need your email address to contact you",
                        email: "Your email address must be in the format of name@domain.com"
                    }
                },

                submitHandler: function (form) {

                    $.ajax({
                         url: '@Url.Action("Create")',
                         type: "POST",
                         data: $(form).serialize(),
                         //data: JSON.stringify({ model }),
                         dataType: 'json',
                         //contentType: 'application/json',
                         success: function (data) {
                                // Inserting html into the result div on success
                                $('#results').html(data);
                            },
                            error: function (jqXHR, text, error) {
                                // Displaying if there are any errors
                                $('#result').html(error);
                            }
                        });
                        return false;
            },


                highlight: function (element) {
                    $(element).closest('.form-group').removeClass('has-success').addClass('has-error');
                },
                unhighlight: function (element) {
                    $(element).closest('.form-group').removeClass('has-error').addClass('has-success');
                },
                errorElement: 'span',
                errorClass: 'help-block',
                errorPlacement: function (error, element) {
                    if (element.parent('.input-group').length) {
                        error.insertAfter(element.parent());
                    } else {
                        error.insertAfter(element);
                    }
                }
            });


            $('#bootstrap-wizard-1').bootstrapWizard({
                'tabClass': 'form-wizard',
                'onNext': function (tab, navigation, index) {
                    var $valid = $("#wizard-1").valid();
                    if (!$valid) {
                        $validator.focusInvalid();
                        return false;
                    } else {
                        $('#bootstrap-wizard-1').find('.form-wizard').children('li').eq(index - 1).addClass(
                            'complete');
                        $('#bootstrap-wizard-1').find('.form-wizard').children('li').eq(index - 1).find('.step')
                            .html('<i class="fa fa-check"></i>');
                    }
                }
            });


            // fuelux wizard
            var wizard = $('.wizard').wizard();

            wizard.on('finished', function (e, data) {
                //$("#fuelux-wizard").submit();
                //console.log("submitted!");
                $.smallBox({
                    title: "Congratulations! Your form was submitted",
                    content: "<i class='fa fa-clock-o'></i> <i>1 seconds ago...</i>",
                    color: "#5F895F",
                    iconSmall: "fa fa-check bounce animated",
                    timeout: 4000
                });

            });



        })

    </script>

    <script src='~/lib/waitMe/waitMe.min.js'></script>
    <script src='~/js/site.js'></script>
    @*<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jstree/3.3.5/themes/default/style.min.css" />
        <script src="//cdnjs.cloudflare.com/ajax/libs/jstree/3.3.5/jstree.min.js"></script>*@


    <script type="text/javascript">
        $(document).ready(function () {

           $("#ciudad").change(function () {
            mostrarLoadingPanel("content","Cargando...")
            $("#sucursal").empty();
            $("#dependencia").val("");
            $("#manualpuesto").empty();
            //$("#rolpuesto").empty();
            //$("#escalagrados").empty();

           $("#txt_escalaGrados").val("");
           $("#txt_rolPuesto").val("");
           $("#txt_idIO").val("0");

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarSucursalesPorCiudad", "CeseFunciones")',
                    dataType: 'json',
                    data: { idciudad: $("#ciudad").val() },
                    success: function (data) {
                        $("#sucursal").append('<option value="0" selected="" disabled="">Seleccione</option>');
                        $.each(data, function (i, data) {
                            $("#sucursal").append('<option value="'
                                + data.idSucursal + '">'
                                + data.nombre + '</option>');

                        });
                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
                })

            $("#sucursal").change(function (e) {
               mostrarLoadingPanel("content", "Cargando...")
               $("#dependencia").val("");
               $("#manualpuesto").empty();
               //$("#rolpuesto").empty();
               //$("#escalagrados").empty();

                $("#txt_escalaGrados").val("");
                $("#txt_rolPuesto").val("");
                $("#txt_idIO").val("0");

               e.preventDefault();
               var $link = $(this);
               var title = $link.text();
               $('#myModal.modal-title').html(title);
               var url = '@Url.Action("CargarDependencias")?idsucursal='+ $('#sucursal').val() ;
               if (url.indexOf('#') == 0) {
                   $('#myModal').modal('show');
                   $("#myModal").waitMe("hide");
               }
               else {
                   $.get(url, function (data) {
                       $('#myModal .te').html(data);
                       $('#myModal').modal();
                       $('#selectC').select2();
                       $("#content").waitMe("hide");
                   }).success(function () { $('input:text:visible:first').focus(); });

               }
                $("#content").waitMe("hide");
                return false;
           })




           $("#manualpuesto").change(function () {
                 mostrarLoadingPanel("content","Cargando...")
                //$("#rolpuesto").empty();
                //$("#escalagrados").empty();

               $("#txt_escalaGrados").val("");
               $("#txt_rolPuesto").val("");
               $("#txt_idIO").val("0");

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarRolPuestoporManualPuesto")',
                    dataType: 'json',
                    data: {
                        idmanualpuesto: $("#manualpuesto").val(),
                        iddependencia: $("#iddependencia").val(),
                        idrelacionLaboral: $("#IdRelacionLaboral").val()
                        },
                    success: function (data) {
                        


                        var textoEscalaGrados = data.indiceOcupacionalViewModel.nombreEscalaGrados
                            + " ( GRADO "
                            + data.indiceOcupacionalViewModel.grado
                            + " ) QUE PERCIBE $"
                            + data.indiceOcupacionalViewModel.remuneracion;


                        $("#txt_escalaGrados").val(textoEscalaGrados);
                        $("#txt_rolPuesto").val(data.indiceOcupacionalViewModel.nombreRolPuesto);
                        $("#txt_idIO").val(data.indiceOcupacionalViewModel.idIndiceOcupacional);


                        if (document.getElementById("s_modalidad").style.visibility.toString() == "hidden") {
                            // si contrato entonces :

                            document.getElementById("txt_partida").value = "";
                            document.getElementById("txt_partida").readOnly = false;
                        } else {
                            // si es  no es contrato (nombramiento) entonces:

                            if (data.numeroPartidaIndividual != null && data.numeroPartidaIndividual.length > 0) {

                                document.getElementById("txt_partida").value = data.numeroPartidaIndividual;
                                document.getElementById("txt_partida").readOnly = true;
                                document.getElementById("modalidadpartida").selectedIndex = data.idModalidadPartida;

                            } else {
                                mostrarNotificacionTimer("Aviso", "No existen puestos vacantes, si a\u00FAn desea ingresar el registro, deber\u00E1 ingresar un N\u00FAmero de partida", 30000);
                                document.getElementById("txt_partida").value = "";
                                document.getElementById("txt_partida").readOnly = false;
                            }



                        }


                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
                        })


                         

        $("#IdRegimenLaboral").change(function () {
            mostrarLoadingPanel("content","Cargando...")
            $("#IdRelacionLaboral").empty();
            $("#IdModalidadPartida").empty();
            $("#IdTipoNombramiento").empty();
            //$("#rolpuesto").empty();
            //$("#escalagrados").empty();
            $("#manualpuesto").empty();

            $("#txt_escalaGrados").val("");
            $("#txt_rolPuesto").val("");
            $("#txt_idIO").val("0");

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarRelacionesLaboralesPorRegimen")',
                    dataType: 'json',
                    data: { regimen: $("#IdRegimenLaboral").val() },
                    success: function (data) {
                        $("#IdRelacionLaboral").append('<option value="0" selected="" disabled="">Seleccione</option>');
                        $.each(data, function (i, data) {

                            $("#IdRelacionLaboral").append('<option value="'
                                + data.idRelacionLaboral + '">'
                                + data.nombre + '</option>');

                        });

                        var stt = data[0].nombre + "";

                        if (stt.toUpperCase() == "CONTRATO") {

                            document.getElementById("s_modalidad").style.visibility = "hidden";
                            document.getElementById("txt_partida").value = "";
                            document.getElementById("txt_partida").readOnly = false;

                        } else {

                            document.getElementById("s_modalidad").style.visibility = "visible";

                            document.getElementById("txt_partida").value = "";
                            document.getElementById("txt_partida").readOnly = true;
                            document.getElementById("modalidadpartida").selectedIndex = data.idModalidadPartida;
                        }



                        $("#txt_escalaGrados").val("");
                        $("#txt_rolPuesto").val("");
                        $("#txt_idIO").val("0");

                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
        })


        $("#IdRelacionLaboral").change(function () {
            mostrarLoadingPanel("content","Cargando tipos de nombramiento...")
            $("#IdTipoNombramiento").empty();
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarTipoNombramientoRelacion")',
                    dataType: 'json',
                    data: { relacion: $("#IdRelacionLaboral").val() },
                    success: function (data) {
                        $("#IdTipoNombramiento").append('<option value="0" selected="" disabled="">Seleccione</option>');
                        $.each(data, function (i, data) {
                            $("#IdTipoNombramiento").append('<option value="'
                                + data.idTipoNombramiento + '">'
                                + data.nombre + '</option>');

                        });


                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
        })

        });

        function isEmpty(obj, propName) {
            return Object.keys(obj[propName]).length == 0;
