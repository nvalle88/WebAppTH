@model bd.webappth.entidades.ViewModels.ViewModelSeleccionPersonal
<link href="~/css/tabla/table.css" rel="stylesheet" />
@using bd.webappth.entidades.ViewModels
@{ var modelo = new ViewModelSeleccionPersonal {iddependecia=Model.iddependecia ,IdPartidaFase = Model.IdPartidaFase, OpcionMenu = 0 }; }

@{await Html.RenderPartialAsync("_PartialViewMenu", modelo);}
<!-- MAIN CONTENT -->
<div id="content">
    
    <p></p>
    <a asp-action="Index">
        <button type="button" asp-action="Index" asp-controller="SeleccionPersonalTalentoHumano" class="btn btn-labeled btn-default">
            <span class="btn-label">
                <i class="glyphicon glyphicon-arrow-left"></i>
            </span>Atr&aacute;s

        </button>
    </a>
    
    <p> </p>
    <!-- widget grid -->
    <section id="widget-grid" class="">

        <!-- START ROW -->
        <div class="row">
            <!-- NEW COL START -->
            <article class="col-sm-12 col-md-12 col-lg-12">
                <!-- Widget ID (each widget will need unique ID)-->
                <div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false" data-widget-custombutton="false">

                    <header>
                        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                        <h2>Informaci&oacute;n Basica</h2>
                    </header>
                    <!-- widget div-->
                    <div>

                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body no-padding">

                            <form asp-action="Create" id="checkout-form" class="smart-form" novalidate="novalidate">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="hidden" asp-for="iddependecia" />
                                @{await Html.RenderPartialAsync("_PartialViewPostulante", Model);}

                                @*<footer>
                                        <input type="submit" value="Guardar" class="btn btn-primary" />
                                        <a asp-action="Index" class="btn btn-default">Cancelar</a>
                                    </footer>*@
                            </form>
                        </div>
                        <!-- end widget content -->
                    </div>
                    <!-- end widget div -->
                </div>
                <!-- end widget -->

            </article>

        </div>
        <!-- END ROW -->
    </section>
    <!-- end widget grid -->
    <!-- Modal -->

</div>
<!-- END MAIN CONTENT -->
@section Scripts {

}