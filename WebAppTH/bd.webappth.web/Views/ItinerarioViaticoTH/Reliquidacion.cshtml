@model bd.webappth.entidades.ViewModels.ViewModelsSolicitudViaticos
<link href="~/css/tabla/table.css" rel="stylesheet" />
@{
    ViewData["Title"] = "Index";
}
<!-- MAIN CONTENT -->
<div id="content">
    <div class="col col-10">

        @if (@ViewData["Error"].ToString() != "")

        {

            <div class="col-lg-12">

                <div class="alert alert-block alert-dismissable">

                    <a class="close" data-dismiss="alert" href="#"><p>X</p></a>

                    <h4 class="alert-heading"><i class="fa fa-check-square-o"></i>Informaci&oacute;n!</h4>

                    <p>

                        <span class="text-danger"><strong><bold> @ViewData["Error"]</bold></strong></span>

                    </p>

                </div>

            </div>

        }
    </div>

    <a asp-action="Index" asp-route-IdSolicitudViatico="@Model.IdSolicitudViatico">
        <button type="button" asp-action="Index" asp-route-IdSolicitudViatico="@Model.IdSolicitudViatico" class="btn btn-labeled btn-default">
            <span class="btn-label">
                <i class="glyphicon glyphicon-arrow-left"></i>
            </span>Regresar
        </button>
    </a>
    <!-- widget grid -->
    <section id="" class="">
        <!-- row -->
        <div class="row">
            <article class="col-lg-12">
                <legend>Reliquidaci&oacute;n Vi&aacute;tico</legend>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Fecha de solicitud:</label>
                            <input asp-for="FechaSolicitud" disabled="disabled" type="text" class="form-control input datepicker " placeholder="Fecha: " data-dateformat='dd/mm/yy' data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label>Fondo de Financiamiento:</label>
                            <input asp-for="FondoFinanciamiento" disabled="disabled" class="form-control input " placeholder="FondoFinanciamiento" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>
                </div>
            </article>
            <article class="col-lg-12">
                <legend>Datos Generales</legend>

                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label>Nombres - Apellidos del Servidor:</label>
                            <input asp-for="Servidor" disabled="disabled" class="form-control input " placeholder="NombreApellido" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label>Nombre de la unidad del servidor:</label>
                            <input asp-for="Dependencia" disabled="disabled" class="form-control input " placeholder="RolPuesto" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label>Puesto:</label>
                            <input asp-for="Puesto" disabled="disabled" class="form-control input " placeholder="RolPuesto" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-4">
                        <label>Pa&iacute;s de comisi&oacute;n:</label>
                        <div class="form-group">
                            <input value="@Model.Pais" disabled class="form-control input " placeholder="NombreApellido" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>

                    <div class="col-sm-4">
                        <label>Provincia de comisi&oacute;n:</label>
                        <div class="form-group">
                            <div class="form-group">
                                <input value="@Model.Provincia" disabled class="form-control input " placeholder="NombreApellido" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <label>Ciudad de comisi&oacute;n:</label>
                        <div class="form-group">
                            <div class="form-group">
                                <input value="@Model.Ciudad" disabled class="form-control input " placeholder="NombreApellido" type="text" data-msg-required='El campo es requerido' data-rule-required='true'>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Fecha de salida:</label>
                            <input asp-for="FechaSalida" disabled="disabled" type="text" class="form-control input datepicker " placeholder="Fecha de salida: " data-dateformat='dd/mm/yy' data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Hora de salida:</label>
                            <input class="form-control" asp-for="HoraSalida" disabled="disabled" id="timepicker1" type="text" placeholder="Seleccione la hora">
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Fecha de llegada:</label>
                            <input asp-for="FechaLlegada" disabled="disabled" type="text" class="form-control input datepicker " placeholder="Fecha de llegada: " data-dateformat='dd/mm/yy' data-msg-required='El campo es requerido' data-rule-required='true'>
                        </div>
                    </div>

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label>Hora de llegada:</label>
                            <input class="form-control" asp-for="HoraLlegada" disabled="disabled" id="timepicker2" type="text" placeholder="Seleccione la hora">
                        </div>
                    </div>
                </div>
                @*<section>
                        <label class="label"></label>
                        <div class="form-group">
                            <label>Informe de actividades y productos alcanzados:</label>
                            <textarea maxlength="250" class="form-control" asp-for="Descripcion" rows="3" placeholder="@Html.DisplayNameFor(model=>model.SolicitudViatico.Descripcion)"></textarea>
                            <span asp-validation-for="Descripcion" class="text-danger"></span>
                        </div>
                    </section>*@
            </article>
        </div>
        <div class="row">
            <section class="col col-lg-12">
                <label class="label"></label>
                <div class="form-group">
                    <label>Informe de actividades y productos alcanzados:</label>
                    <textarea maxlength="250" class="form-control" asp-for="Descripcion" disabled="disabled" rows="3" placeholder="@Html.DisplayNameFor(model=>model.Descripcion)"></textarea>
                    <span asp-validation-for="Descripcion" class="text-danger"></span>
                </div>
            </section>
        </div>
        <div class="row">
            @if (Model.Estado == 4)
            {
                <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <a asp-action="CreateReliquidacion" asp-route-id="@Model.IdSolicitudViatico">
                        <button type="button" asp-action="CreateReliquidacion" asp-route-id="@Model.IdSolicitudViatico" class="btn btn-labeled btn-default">
                            <span class="btn-label">
                                <i class="glyphicon glyphicon-plus"></i>
                            </span>Nuevo
                        </button>
                    </a>
                </article>
            }
        </div>

        <div class="row">
            <!-- NEW WIDGET START -->
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <!-- Widget ID (each widget will need unique ID)-->
                <div class="jarviswidget jarviswidget-color-darken" id="wid-id-0" data-widget-editbutton="false">
                    <!-- widget options:
                    usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">

                    data-widget-colorbutton="false"
                    data-widget-editbutton="false"
                    data-widget-togglebutton="false"
                    data-widget-deletebutton="false"
                    data-widget-fullscreenbutton="false"
                    data-widget-custombutton="false"
                    data-widget-collapsed="true"
                    data-widget-sortable="false"

                    -->
                    <br />
                    <header id="table">
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Transporte Utilizado</h2>
                    </header>
                    <!-- widget div-->
                    <div>
                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->
                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body no-padding">
                            <table id="datatable_fixed_column" class="table table-striped table-bordered table-hover" width="100%">
                                <thead>
                                    <tr>
                                        <th data-class="expand">
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().TipoTransporte.Descripcion)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().NombreTransporte)
                                        </th>
                                        <th>
                                            Ruta
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().FechaSalida)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().HoraSalida)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().FechaLlegada)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().HoraLlegada)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.ListaReliquidacionViatico.FirstOrDefault().ValorEstimado)
                                        </th>
                                        <th>Opciones</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.ListaReliquidacionViatico)
                                    {
                                        <tr>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.TipoTransporte.Descripcion)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.NombreTransporte)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.CiudadOrigen.Nombre) - @Html.DisplayFor(modelItem => item.CiudadDestino.Nombre)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FechaSalida)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.HoraSalida)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FechaLlegada)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.HoraLlegada)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.ValorEstimado)
                                            </td>

                                            <td>
                                                @if (Model.Estado == 4)
                                                {
                                                    <text>
                                                        @Html.ActionLink("Editar", "EditReliquidacion", new { id = item.IdReliquidacionViatico, IdSolicitudViatico = Model.IdSolicitudViatico }) |
                                                        @Html.ActionLink("Eliminar", "DeleteReliquidacion", new { id = item.IdReliquidacionViatico, IdSolicitudViatico = Model.IdSolicitudViatico }, new { onclick = "return confirm('Desea eliminar el itinerario del viático... ?');" })
                                                    </text>
                                                }
                                                @*@if (Model.EstadoReliquidacion == 1)
                                                {
                                                    <text>
                                                        @Html.ActionLink("Solicitud Pago", "ReporteSolicitudPagoReliquidacion", "Report", new { idReliquidacionViatico = item.IdReliquidacionViatico }, new { onclick = "cargando();" })

                                                    </text>
                                                }*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>

                        <!-- end widget content -->
                    </div>
                    <!-- end widget div -->
                </div>

            </article>
            <!-- WIDGET END -->
        </div>


        <div class="row">
            @if (Model.Estado == 4)
            {
            <form id="wizard-1" asp-action="FinalizarReliquidacion" novalidate="novalidate">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="IdSolicitudViatico" />               
                <input type="hidden" asp-for="ListaReliquidacionViatico.FirstOrDefault().ValorRequlidacion" value="@Model.ListaReliquidacionViatico" />
                <input type="hidden" asp-for="ListaReliquidacionViatico.FirstOrDefault().ValorTotalRequlidacion" value="@Model.ListaReliquidacionViatico" />
                <div class="row col col-lg-12">
                    <section class="col col-lg-6">
                        <div class="form-group">
                            <label>Partida Presupuestaria:</label>
                            <select asp-for="ListaReliquidacionViatico.FirstOrDefault().IdPresupuesto" class="form-control input" asp-items="ViewBag.IdPresupuesto" data-msg-required='El campo es requerido' data-rule-required='true'>
                                <option value="0" selected="" disabled="">Seleccione</option>
                            </select>
                        </div>
                    </section>
                    <section class="col col-lg-6">
                        <div class="form-group">
                            <label>Valor reliquidacion:</label>
                            <input asp-for="Valor" disabled="disabled" type="text" class="form-control">
                        </div>
                    </section>
                </div>
                <div class="row col col-lg-12">
                    <section class="col col-lg-11">
                        &nbsp;
                    </section>

                    <section class="col col-lg-1">
                        <label class="label"></label>
                        <div class="form-group">
                            <input type="submit" value="Finalizar" class="btn btn-primary" />
                        </div>
                    </section>
                </div>
            </form>
            }
        </div>
        <!-- end row -->
        <!-- end row -->
    </section>
    <!-- end widget grid -->
</div>
<!-- END MAIN CONTENT -->

@section Scripts {
    <script src='~/js/datatables/init.js'></script>
}