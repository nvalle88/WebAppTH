@section Scripts {
    <!-- PAGE RELATED PLUGIN(S) -->

    <script src='~/lib/select2/dist/js/select2.min.js'></script>
    <script src='~/lib/select2/dist/js/i18n/es.js'></script>
    <script src='~/lib/waitMe/waitMe.min.js'></script>
    <script src='~/js/site.js'></script>
    <script src='~/js/vistasmodales/modal.js'></script>

    <script src="~/js/plugin/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
    <script src="~/js/plugin/fuelux/wizard/wizard.min.js"></script>



    <script type="text/javascript">
        $(document).ready(function () {

            $("#idgrupoocupacional").change(function () {

                ListarEscalasGradosPorGrupoOcupacional($("#idgrupoocupacional").val());
                return false;
            });

            function ListarEscalasGradosPorGrupoOcupacional(id) { 

                mostrarLoadingPanel("content", "Cargando...")
                $("#idescalagrados").empty();

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ListarEscalasGradosPorGrupoOcupacional", "EscalaGrados")',
                    dataType: 'json',
                    data: { idgrupoocupacional: id },
                    success: function (data) {
                        $("#idescalagrados").append('<option value="0" selected="" disabled="">Seleccione</option>');

                        $.each(data, function (i, data) {
                            $("#idescalagrados").append('<option value="'
                            + data.idEscalaGrados + '">'
                            + data.nombre + '</option>');
                        });
                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });

            }


            $("#idescalagrados").change(function () {

                
                ObtenerDatosEscalaGradosPorId($("#idescalagrados").val());

                
                return false;
            });


            function ObtenerDatosEscalaGradosPorId( id ) { 

                mostrarLoadingPanel("content", "Cargando...");

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("ObtenerDatosEscalaGradosPorId", "IndicesOcupacionales")',
                    dataType: 'json',
                    data: { id: id },
                    success: function (data) {

                        $('#txt_rmu').val("$"+data.remuneracion);

                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });

            }

            
            ListarEscalasGradosPorGrupoOcupacional(@(Model.IndiceOcupacional.EscalaGrados.IdGrupoOcupacional));
            $("#idgrupoocupacional").val(@(Model.IndiceOcupacional.EscalaGrados.IdGrupoOcupacional));


            ObtenerDatosEscalaGradosPorId(@(Model.IndiceOcupacional.IdEscalaGrados));

            $("#idescalagrados").val(@(Model.IndiceOcupacional.IdEscalaGrados));
        });


    </script>
    <script src='~/lib/select2/dist/js/select2.min.js'></script>
    <script src='~/lib/select2/dist/js/i18n/es.js'></script>
    <script src='~/lib/waitMe/waitMe.min.js'></script>
    @* <script src='~/js/site.js'></script>*@
    <script type="text/javascript">
        $(document).ready(function () {
            Init_Select2();
        });
    </script>

}




