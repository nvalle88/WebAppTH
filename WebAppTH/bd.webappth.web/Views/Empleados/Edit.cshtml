    <script type="text/javascript">
    $(document).ready(function () {

        $("#etniaE").change(function () {


            mostrarLoadingPanel("content", "Cargando...")
            $("#nacionalidadIndigenaE").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("ListarNacionalidadIndigena")',
                    dataType: 'json',
                    data: { etnia: $("#etniaE").val() },
                    success: function (data) {
                        $.each(data, function (i, data) {
                            $("#nacionalidadIndigenaE").append('<option value="'
                                + data.idNacionalidadIndigena + '">'
                                + data.nombre + '</option>');

                        });
                    }, complete: function (data) {
                        $("#content").waitMe("hide");
                    },

                    error: function (ex) {
                        alert('Failed to retrieve data.' + ex);
                    }
                });
                return false;
        })

