@model bd.webappth.entidades.ViewModels.AccionPersonalViewModel


<fieldset>
    <input asp-for="IdAccionPersonal" type="text" />
    <input asp-for="IdSeleccionado" type="text" />
    <input asp-for="IdEmpleadoAfectado" type="text" />

    <div class="row">&nbsp;</div>

    <div class="row">

        <section class="col col-lg-4">

            <label class="input col-lg-12">Nombre del funcionario</label>
            <label class="input col-lg-12">
                <input type="text" value="@(Model.DistributivoSituacionActual.Empleado.Persona.Nombres) @(Model.DistributivoSituacionActual.Empleado.Persona.Apellidos)" class="form-control" readonly />
            </label>

        </section>

        <section class="col col-lg-4">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.DistributivoSituacionActual.Empleado.Persona.Identificacion)</label>
            <label class="input col-lg-12">
                <input type="text" asp-for="DistributivoSituacionActual.Empleado.Persona.Identificacion" class="form-control" readonly />
            </label>

        </section>

        <section class="col-lg-4">
            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.TipoAccionPersonalViewModel.IdTipoAccionPersonal)</label>
            <label class="input" style="padding-right:15px;">
                <select id="SelectAccion" asp-for="TipoAccionPersonalViewModel.IdTipoAccionPersonal" class="form-control" asp-items="ViewBag.TipoAcciones">
                    <option value="0" selected>Seleccione</option>
                </select>

                <span asp-validation-for="TipoAccionPersonalViewModel.IdTipoAccionPersonal" class="text-danger"></span>
            </label>
        </section>

    </div>

    <div class="row">&nbsp;</div>

    <div class="row">


        <section class="col col-lg-8">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.Solicitud)</label>
            <label class="input col-lg-12">
                <textarea asp-for="Solicitud" class="form-control" rows="5"></textarea>
                <span asp-validation-for="Solicitud" class="text-danger"></span>
            </label>

        </section>

        <section class="col col-lg-4">
            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.Estado)</label>
            <label class="input">
                <select asp-for="Estado" class="form-control" asp-items="ViewBag.Estados">Seleccione</select>

                <span asp-validation-for="Estado" class="text-danger"></span>
            </label>
        </section>

    </div>

    <div class="row">&nbsp;</div>

    <div class="row">

        <section class="col col-lg-8">
            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.Explicacion)</label>
            <label class="input col-lg-12">
                <textarea maxlength="1000" asp-for="Explicacion" class="form-control" rows="5" style="resize:none;"></textarea>
                <span asp-validation-for="Explicacion" class="text-danger"></span>
            </label>
        </section>


    </div>

    <div class="row">&nbsp;</div>

    <div class="row">
        <section class="col col-lg-12">
            <legend class="input col-lg-12">Generar movimiento de personal?</legend>
        </section>

        <section class="col col-lg-4">
            <div class="input col-lg-12">
                @Html.RadioButtonFor(x => x.TipoAccionPersonalViewModel.GenerarMovimientoPersonal, false, new { onClick = "ValidarChkMovPersonal(this)", @id = "rdbFalse", @disabled = "disabled" })&nbsp;No
            </div>
        </section>

        <section class="col col-lg-4">
            <div class="input col-lg-12">
                @Html.RadioButtonFor(x => x.TipoAccionPersonalViewModel.GenerarMovimientoPersonal, true, new { onClick = "ValidarChkMovPersonal(this)", @id = "rdbTrue", @disabled = "disabled" })&nbsp;Si
            </div>
        </section>

    </div>

    <div class="row divSituacionActual">&nbsp;</div>

    <div class="row divSituacionActual">

        <section class="col col-lg-10 divSituacionActual">
            <legend class="input col-lg-12">Situaci&oacute;n actual</legend>
        </section>

        <section class="col col-lg-6 divSituacionActual">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => model.DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.Dependencia.Sucursal.Nombre)</label>
            <label class="input col-lg-12">
                <input type="text" asp-for="DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.Dependencia.Sucursal.Nombre" class="form-control" readonly />
            </label>

        </section>

        <section class="col col-lg-6 divSituacionActual">

            <label class="input col-lg-12">Unidad administrativa</label>
            <label class="input col-lg-12">
                <input type="text" asp-for="DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.Dependencia.Nombre" class="form-control" readonly />
                <span asp-validation-for="DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.Dependencia.Nombre" class="text-danger"></span>
            </label>

        </section>

        <section class="col col-lg-6 divSituacionActual">

            <label class="input col-lg-12">Nombre del puesto</label>
            <label class="input col-lg-12">
                <input type="text" asp-for="DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.IndiceOcupacional.DenominacionPuesto" class="form-control" readonly />
            </label>

        </section>

        <section class="col-lg-6 divSituacionActual">

            <label class="input col-lg-12">Relaci&oacute;n laboral</label>
            <label class="input col-lg-12">
                <input type="text" value="@Model.DistributivoSituacionActual.TipoNombramiento.RelacionLaboral.Nombre" class="form-control" readonly />

            </label>

        </section>

        <section class="col-lg-2 divSituacionActual">

            <label class="input col-lg-12">Remuneraci&oacute;n</label>
            <label class="input col-lg-12">
                <input type="text" value="$@Model.DistributivoSituacionActual.IndiceOcupacionalModalidadPartida.IndiceOcupacional.EscalaGrados.Remuneracion" class="form-control" readonly />

            </label>

        </section>

    </div>

    <div class="row divSituacionActual">&nbsp;</div>


    <div class="row divSituacionPropuesta">
        <section class="col col-lg-12 divSituacionPropuesta">
            <legend class="input col-lg-12">Situaci&oacute;n propuesta</legend>
        </section>
    </div>

    <div class="row divSituacionPropuesta divPuestosOcupados">

        <section class="col col-lg-12 divSituacionPropuesta divPuestosOcupados" style="padding-left:2%;padding-right:2%;">
            <div class="jarviswidget jarviswidget-color-darken" id="wid-id-100" data-widget-editbutton="false" style="padding:0px;margin:0px;">
                <header id="table">
                    <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                    <h2>Selecci&oacute;n de puestos ocupados</h2>
                </header>

                <table id="datatable_fixed_column" class="table table-striped table-bordered table-hover">

                    <thead>
                        <tr>
                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th class="hasinput">
                                <input type="text" class="form-control" />
                            </th>

                            <th>

                            </th>
                        </tr>

                        <tr>
                            <th data-class="expand">
                                N&uacute;meroPartida
                            </th>

                            <th data-class="expand">
                                C&oacute;digo contrato
                            </th>

                            <th data-class="expand">
                                Servidor
                            </th>

                            <th data-class="expand">
                                &Aacute;rea geogr&aacute;fica
                            </th>

                            <th data-class="expand">
                                Unidad administrativa
                            </th>

                            <th data-class="expand">
                                Grupo ocupacional y remuneraci&oacute;n
                            </th>

                            <th data-class="expand">
                                Fondo de financiamiento
                            </th>

                            <th data-class="expand">
                                Seleccionar
                            </th>

                        </tr>

                    </thead>

                    @{ var lista = new List<bd.webappth.entidades.Negocio.DistributivoSituacionActual>();}

                    <tbody>

                        @foreach (var item in Model.ListaDistributivoSeleccion)
                        {


                            <tr name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.NumeroPartidaIndividual)
                                        &nbsp;
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.ModalidadPartida.Nombre)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.CodigoContrato)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    @{
                                        var datosPersonales = "";

                                        if (item.Empleado != null)
                                        {
                                            datosPersonales = item.Empleado.Persona.Nombres + " " + item.Empleado.Persona.Apellidos + " " + item.Empleado.Persona.Identificacion;
                                        }

                                    }
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @(datosPersonales)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">

                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.Dependencia.Sucursal.Nombre)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.Dependencia.Nombre)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.IndiceOcupacional.EscalaGrados.Nombre)&nbsp;

                                        @Html.DisplayFor(modelItem => item.IndiceOcupacionalModalidadPartida.IndiceOcupacional.EscalaGrados.Remuneracion)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        @Html.DisplayFor(modelItem => item.FondoFinanciamiento.Nombre)
                                    </div>
                                </td>

                                <td name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">

                                    <div name="tr@(item.IndiceOcupacionalModalidadPartida.Ocupado)">
                                        <input type="radio" value="@item.IndiceOcupacionalModalidadPartida.IdIndiceOcupacionalModalidadPartida" name="rdb_seleccionPuesto" onclick="seleccionIOMP(this)"/>
                                    </div>


                                </td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </section>

    </div>

    <div class="row divSituacionPropuesta divNuevoPuesto">
        <section class="col col-lg-10 divSituacionPropuesta divNuevoPuesto">
            <legend class="input col-lg-12" style="font-size:15px;font-weight:500;">Puestos vacantes</legend>
        </section>
    </div>


    <div class="row divSituacionPropuesta">

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">
                <label>N&uacute;mero de partida / c&oacute;digo de contrato</label>
                <select id="sel_numeroPartida" class="form-control" asp-for="IdPartidaVacante" asp-items="ViewBag.PartidasVacantes" data-msg-required='El campo es requerido' data-rule-required='true'>
                    <option value="0" selected="" disabled="">Seleccione</option>
                </select>
            </section>


            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>R&eacute;gimen laboral</label>
                <input type="text" class="form-control input" id="txt_RegimenLaboral" readonly required>
                <input type="text" class="form-control input" asp-for="IdRegimenLaboralHasta" id="txt_IdRegimenLaboral" readonly required>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Relaci&oacute;n laboral</label>
                <input type="text" class="form-control input" id="txt_ReLacionLaboral" readonly required>
                <input type="text" class="form-control input" asp-for="IdRelacionLaboralHasta" id="txt_IdReLacionLaboral" readonly required>
            </section>

        </section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">&nbsp;</section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">


            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">
                <label>Tipo de nombramiento / contrato</label>
                <select id="IdTipoNombramiento" asp-for="IdTipoNombramientoHasta" class="form-control" asp-items="ViewBag.IdTipoNombramiento" data-msg-required='El campo es requerido' data-rule-required='true'>Seleccione</select>
                <span asp-validation-for="IdTipoNombramientoHasta" class="text-danger"></span>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Ciudad</label>
                <input type="text" class="form-control input" id="txt_Ciudad" readonly required>
                <input type="text" class="form-control input" id="txt_IdCiudad" readonly required>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Sucursal</label>
                <input type="text" class="form-control input" id="txt_Sucursal" readonly required>
                <input type="text" class="form-control input" id="txt_IdSucursal" readonly required>
            </section>

        </section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">&nbsp;</section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">


            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">
                <label>Unidad administrativa</label>
                <input type="text" class="form-control input" id="txt_Dependencia" readonly required>
                <input type="text" class="form-control input" id="txt_IdDependencia" readonly required>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Perfil del puesto</label>
                <input type="text" class="form-control input" id="txt_Puesto" readonly required>
                <input type="text" class="form-control input" id="txt_IdPuesto" readonly required>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Rol del puesto</label>
                <input type="text" class="form-control input" id="txt_Rol" readonly required>
                <input type="text" class="form-control input" id="txt_IdRol" readonly required>
            </section>

        </section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">&nbsp;</section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">


            <section class="col-lg-8 divSituacionPropuesta divNuevoPuesto">
                <label>Grupo ocupacional</label>
                <input type="text" class="form-control input" id="txt_GrupoOcupacional" readonly required>
                <input type="text" class="form-control input" id="txt_IdGrupoOcupacional" readonly required>
            </section>

            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto" id="s_modalidad">

                <label>Modalidad Partida</label>
                <select id="modalidadpartida" asp-for="IdModalidadPartidaHasta" asp-items="ViewBag.IdModalidadPartida" class="form-control">Seleccione</select>

            </section>



        </section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">&nbsp;</section>

        <section class="col col-lg-12 divSituacionPropuesta divNuevoPuesto">


            <section class="col-lg-4 divSituacionPropuesta divNuevoPuesto">

                <label>Fondo de financiamiento</label>
                <select id="idFondoFinanciamiento" asp-for="IdFondoFinanciamientoHasta" asp-items="ViewBag.IdFondoFinanciamiento" class="form-control">Seleccione</select>

            </section>



        </section>

    </div>


    <div class="row">

        <section class="col col-lg-12">
            <legend>&nbsp;</legend>
        </section>

        <section class="col col-lg-4">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.FechaSolicitud)</label>
            <label class="input col-lg-12">
                <input asp-for="FechaSolicitud" type="text" class="form-control datepicker" data-dateformat='yy-mm-dd' />
                <span asp-validation-for="FechaSolicitud" class="text-danger"></span>
            </label>

        </section>

        <section class="col col-lg-4">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.FechaRige)</label>
            <label class="input col-lg-12">
                <input id="date_Fecha1" asp-for="FechaRige" type="text" class="form-control datepicker" data-dateformat='yy-mm-dd' onchange="calcularDias()" />
                <span asp-validation-for="FechaRige" class="text-danger"></span>
            </label>

        </section>

        <section class="col col-lg-4">

            <label class="input col-lg-12" id="lbl_date_Fecha2">@Html.DisplayNameFor(model => Model.FechaRigeHasta)</label>
            <label class="input col-lg-12">
                <input id="date_Fecha2" asp-for="FechaRigeHasta" type="text" class="form-control datepicker" data-dateformat='yy-mm-dd' onchange="calcularDias()" />
                <span asp-validation-for="FechaRigeHasta" class="text-danger"></span>
            </label>

        </section>

    </div>

    <div class="row">&nbsp;</div>

    <div class="row" id="txt_ver_dias">

        <section class="col col-lg-4">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.TotalDias)</label>
            <label class="input col-lg-12">
                <input id="txt_dias" min="0" readonly type="number" asp-for="TotalDias" class="form-control" />
                <span asp-validation-for="TotalDias" class="text-danger"></span>
            </label>

        </section>

        <section class="col col-lg-4">

            <label class="input col-lg-12">@Html.DisplayNameFor(model => Model.DiasRestantes)</label>
            <label class="input col-lg-12">
                <input id="txt_Numero" min="0" readonly type="number" asp-for="DiasRestantes" class="form-control" />
                <span asp-validation-for="DiasRestantes" class="text-danger"></span>
            </label>

        </section>

    </div>

    <div class="row">&nbsp;</div>
    <div class="row">&nbsp;</div>

</fieldset>

<script>

    function MostrarMovimientoPersonal() {
        var varItemsActual = document.getElementsByClassName("divSituacionActual").length;
        var varItemsPropuesta = document.getElementsByClassName("divSituacionPropuesta").length;

        for (var i = 0; i < varItemsActual; i++) {
            document.getElementsByClassName("divSituacionActual")[i].style.height = "auto";
            document.getElementsByClassName("divSituacionActual")[i].style.visibility = "visible";
        }

        for (var j = 0; j < varItemsPropuesta; j++) {
            document.getElementsByClassName("divSituacionPropuesta")[j].style.height = "auto";
            document.getElementsByClassName("divSituacionPropuesta")[j].style.visibility = "visible";
        }
        
    }

    function OcultarMovimientoPersonal() {

        var varItemsActual = document.getElementsByClassName("divSituacionActual").length;
        var varItemsPropuesta = document.getElementsByClassName("divSituacionPropuesta").length;

        for (var i = 0; i < varItemsActual; i++) {
            document.getElementsByClassName("divSituacionActual")[i].style.height = "0px";
            document.getElementsByClassName("divSituacionActual")[i].style.visibility = "hidden";
        }

        for (var j = 0; j < varItemsPropuesta; j++) {
            document.getElementsByClassName("divSituacionPropuesta")[j].style.height = "0px";
            document.getElementsByClassName("divSituacionPropuesta")[j].style.visibility = "hidden";
        }

    }

    function MostrarNuevoPuesto() {

        var varItemsNuevoPuesto = document.getElementsByClassName("divNuevoPuesto").length;

        for (var i = 0; i < varItemsNuevoPuesto; i++) {
            document.getElementsByClassName("divNuevoPuesto")[i].style.height = "auto";
            document.getElementsByClassName("divNuevoPuesto")[i].style.visibility = "visible";
            
        }
        
    }

    function OcultarNuevoPuesto() {

        var varItemsNuevoPuesto = document.getElementsByClassName("divNuevoPuesto").length;

        for (var i = 0; i < varItemsNuevoPuesto; i++) {
            document.getElementsByClassName("divNuevoPuesto")[i].style.height = "0px";
            document.getElementsByClassName("divNuevoPuesto")[i].style.visibility = "hidden";
        }

    }

    function MostrarPuestosOcupados() {

        var varItemsPuestosOcupados = document.getElementsByClassName("divPuestosOcupados").length;

        for (var i = 0; i < varItemsPuestosOcupados; i++) {
            document.getElementsByClassName("divPuestosOcupados")[i].style.height = "auto";
            document.getElementsByClassName("divPuestosOcupados")[i].style.visibility = "visible";
        }
    }

    function OcultarPuestosOcupados() {

        var varItemsPuestosOcupados = document.getElementsByClassName("divPuestosOcupados").length;

        for (var i = 0; i < varItemsPuestosOcupados; i++) {
            document.getElementsByClassName("divPuestosOcupados")[i].style.height = "0px";
            document.getElementsByClassName("divPuestosOcupados")[i].style.visibility = "hidden";
        }
    }

    


    function ValidarChkMovPersonal(element) {


        if (element.value == "True") {
            MostrarMovimientoPersonal();
        }

        else if (element.value == "False") {
            OcultarMovimientoPersonal();
        }

    }


    function ValidarNuevoPuesto(element) {

        if (element.value == "True") {
            MostrarNuevoPuesto();
            OcultarPuestosOcupados();
        }

        else if (element.value == "False") {
            OcultarNuevoPuesto();
            MostrarPuestosOcupados();
        }

    }

    
    function seleccionIOMP(element) { 

        document.getElementById("IdSeleccionado").value = element.value;
        
        $("#sel_numeroPartida").val("0");
        $("#IdTipoNombramiento").empty();

        $("#txt_RegimenLaboral").val(null);
        $("#txt_IdRegimenLaboral").val(null);

        $("#txt_ReLacionLaboral").val(null);
        $("#txt_IdReLacionLaboral").val(null);

        $("#txt_Ciudad").val(null);
        $("#txt_IdCiudad").val(null);

        $("#txt_Sucursal").val(null);
        $("#txt_IdSucursal").val(null);


        $("#txt_Dependencia").val(null);
        $("#txt_IdDependencia").val(null);

        $("#txt_Puesto").val(null);
        $("#txt_IdPuesto").val(null);

        $("#txt_Rol").val(null);
        $("#txt_IdRol").val(null);

        var textoEscalaGrados = ""

        $("#txt_GrupoOcupacional").val(textoEscalaGrados);
        $("#txt_IdGrupoOcupacional").val(null);

        document.getElementById("s_modalidad").style.visibility = "hidden";
        document.getElementById("s_modalidad").style.height = "0px";

    }

    function setDefaultModalidadPartida(optionDefault) { 
        
        var lista = document.getElementById("modalidadpartida").options;

        for (var i = 0; i < lista.length; i++) {

            var varTexto = String(lista[i].text).toUpperCase();
            var varDefault = String(optionDefault).toUpperCase();

            if (varTexto == varDefault ) {
                document.getElementById("modalidadpartida").value = lista[i].value;
                break;
            }
        }

    }

    function setModalidadPartida(IdModalidadPartida) {
        document.getElementById("modalidadpartida").value = IdModalidadPartida;
    }

    function setTipoNombramiento(IdTipoNombramiento) {
        document.getElementById("IdTipoNombramiento").value = IdTipoNombramiento;
    }

    function checkRadioSeleccionPartida(valor) { 

        var lista = document.getElementsByName("rdb_seleccionPuesto");

        for (var i = 0; i < lista.length; i++) {

            var varValue = String(lista[i].value);

            if (varValue == valor) {
                document.getElementsByName("rdb_seleccionPuesto")[i].checked = true;
                break;
            } 
        }
    }

    setDefaultModalidadPartida("ocupada");


    @if (Model.FechaRigeHasta == null)
        {
            <text>
                document.getElementById("date_Fecha2").style.visibility = "hidden";
                document.getElementById("lbl_date_Fecha2").style.visibility = "hidden";
            </text>
        }
        else {
            <text>
                document.getElementById("date_Fecha2").style.visibility = "visible";
                document.getElementById("lbl_date_Fecha2").style.visibility = "visible";
            </text>
        }

    @{ 
        if (Model.IdAccionPersonal > 0)
        {
            
        }
        else { 
            <text>
                OcultarMovimientoPersonal();
            </text>
        }

    }

</script>


